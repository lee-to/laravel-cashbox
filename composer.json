{
    "name": "lee-to/laravel-cashbox",
    "description": "Laravel cashbox module",
    "keywords": ["laravel", "cashbox"],
    "type": "library",
    "homepage": "https://github.com/lee-to/laravel-cashbox",
    "require": {
        "php": "^7.3",
		"telegram-bot/api": "^2.3",
        "yoomoney/yookassa-sdk-php": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "8.5.x-dev",
        "mockery/mockery": "^1.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Danil Shutsky",
            "email": "leetodev@ya.ru",
            "homepage": "https://github.com/lee-to"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Leeto\\CashBox\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Leeto\\CashBox\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Leeto\\CashBox\\Providers\\CashBoxServiceProvider"
            ]
        }
    }
}
